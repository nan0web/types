{
	"name": "@nan0web/types",
	"version": "1.0.3",
	"description": "Universal system for managing data structures and types in JavaScript applications based on the nan0 philosophy where zero represents the universe.",
	"type": "module",
	"main": "src/index.js",
	"types": "types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./src": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./src/*": {
			"types": "./types/*.d.ts",
			"import": "./src/*.js"
		}
	},
	"files": [
		"src/**/*.js",
		"!src/**/*.test.js",
		"types/**/*.d.ts"
	],
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"prepublishOnly": "npm test && npm run build",
		"playground": "node playground/main.js",
		"release": "nan0release publish",
		"test": "node --test",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:status": "nan0test status --hide-name"
	},
	"keywords": [
		"nan0",
		"data",
		"types",
		"utilities",
		"object",
		"array",
		"validation",
		"format",
		"parser"
	],
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"husky": "^9.1.7",
		"@nan0web/llimo": "workspace:*",
		"@nan0web/release": "workspace:*",
		"@nan0web/test": "workspace:*",
		"@nan0web/ui-cli": "workspace:*"
	}
}
