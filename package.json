{
	"name": "@nan0web/types",
	"version": "1.0.0",
	"description": "Universal system for managing data structures and types in JavaScript applications based on the nan0 philosophy where zero represents the universe.",
	"type": "module",
	"main": "src/index.js",
	"types": "types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./src": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		},
		"./src/*": {
			"types": "./types/*.d.ts",
			"import": "./src/*.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"test": "node --test",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky"
	},
	"keywords": [
		"nan0",
		"data",
		"types",
		"utilities",
		"object",
		"array",
		"validation",
		"format",
		"parser"
	],
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"husky": "^9.1.7"
	}
}
